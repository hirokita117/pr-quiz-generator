# GitHub API設定
VITE_GITHUB_API_URL=https://api.github.com
VITE_GITHUB_TOKEN=your_github_personal_access_token_here

# OpenAI API設定
VITE_OPENAI_API_KEY=your_openai_api_key_here
VITE_OPENAI_API_URL=https://api.openai.com/v1

# Google AI API設定
VITE_GOOGLE_API_KEY=your_google_ai_api_key_here
VITE_GOOGLE_API_URL=https://generativelanguage.googleapis.com/v1

# ローカルLLM設定
VITE_LOCAL_LLM_ENDPOINT=http://localhost:11434
VITE_LOCAL_LLM_MODEL=llama2

# アプリケーション設定
VITE_APP_TITLE=PR Quiz Generator
VITE_APP_VERSION=1.0.0
VITE_DEFAULT_AI_PROVIDER=openai
VITE_DEFAULT_QUESTION_COUNT=10
VITE_DEFAULT_DIFFICULTY=mixed

# キャッシュ設定
VITE_CACHE_ENABLED=true
VITE_CACHE_TTL=3600000
VITE_CACHE_MAX_SIZE=100

# ログ設定
VITE_LOG_LEVEL=info
VITE_DEBUG_MODE=false
